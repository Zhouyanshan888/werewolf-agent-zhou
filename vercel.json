{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": { "runtime": "python-3.11.9" }
    }
  ],
  "functions": {
    "main.py": {
      "runtime": "python-3.11.9",
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
